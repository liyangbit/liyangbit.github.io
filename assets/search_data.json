---
---
[
  {% for pg in site.pages %}
  {% if pg.title != nil %}
    {
      "title"    : "{{ pg.title | escape }}",
      "url"      : "{{ site.url }}{{ pg.url }}"
    } ,
  {% endif %}
  {% endfor %}

  {% for doc in site.documents %}
    {
      "title"    : "{{ doc.title | escape }}",
      "category" : "{{ doc.categories[0] | xml_escape }}",
      "url"      : "{{ site.url }}{{ doc.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
